<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Quiz</h1>
      <ul class="list-group mb-3">
        <% @quizzes.each do |quiz| %>
          <li class="list-group-item">
            <%= link_to quiz.name, quiz_path(quiz) %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
