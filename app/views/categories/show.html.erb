<div class= "home" style="background-image: url('<%= asset_path "foto_home.png" %>')">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-sm-10 col-sm-offset-1">
        <ul class="list-group mb-3">
          <%  @category.products.each do |product| %>
            <li class="list-group-item">
              <%= product.name %>
                <% if product.photo.attached? %>
                  <%= cl_image_tag product.photo.key, height: 200, width: 200, crop: :fill %>
                <% end %>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>

